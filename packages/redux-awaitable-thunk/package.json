{
  "name": "redux-awaitable-thunk",
  "version": "1.0.0",
  "description": "A redux middleware that allows you to await completion of other in-progress thunks, allowing for complex flow control within thunks.",
  "main": "dist/awaitableThunkMiddleware.cjs.js",
  "module": "dist/awaitableThunkMiddleware.esm.js",
  "repository": "github:itaylor/thunk-centric-redux",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run lint && node node_modules/.bin/rollup -c",
    "dev": "node node_modules/.bin/rollup -c -w",
    "test": "node node_modules/.bin/jest",
    "lint": "./node_modules/.bin/eslint src --max-warnings 0",
    "pretest": "yarn run build",
    "coverage": "node node_modules/.bin/jest --coverage",
    "precoverage": "yarn run build"
  },
  "keywords": [
    "redux",
    "middleware",
    "thunk",
    "flow-control",
    "thunk",
    "await"
  ],
  "author": "ian.b.taylor@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^23.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.2",
    "jest": "^23.1.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rollup": "^0.60.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3"
  },
  "dependencies": {}
}
